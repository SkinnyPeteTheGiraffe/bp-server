{
  "name": "bp-server",
  "version": "1.0.0",
  "description": "The official server for Bobby Plunkett's website",
  "main": "dist/app.js",
  "author": "Bobby Plunkett",
  "private": true,
  "scripts": {
    "start": "tsc && node dist/app.js",
    "dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": 3,
    "execMap": {
      "ts": "yarn run start"
    }
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/express-jwt": "^6.0.1",
    "@types/figlet": "^1.2.1",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.22",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "apollo-server": "^2.21.1",
    "apollo-server-express": "^2.19.2",
    "bcrypt": "^5.0.0",
    "chalk": "^4.1.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "figlet": "^1.5.0",
    "graphql": "^15.5.0",
    "graphql-scalars": "^1.7.0",
    "graphql-tools": "^7.0.2",
    "graphql-type-json": "^0.3.2",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongodb": "^3.6.3",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.1.0",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2",
    "typeorm-transactional-cls-hooked": "^0.1.20",
    "typeorm-typedi-extensions": "^0.4.1"
  }
}
